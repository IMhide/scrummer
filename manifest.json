{
	"name": "Scrummer",
	"version": "0.8",
	"description": "Adds Storypoints to Trello",
	"content_scripts": [
		{
      "run_at": "document_end",
			"matches": ["https://trello.com/*"],
			"js": ["scrummer.js"],
			"css": ["scrummer.css"]
		}
	],
	"manifest_version" : 2,
	"icons": {
		"48": "icon48.png",
		"128": "icon128.png",
		"1024": "icon1024.png"
	}
}
